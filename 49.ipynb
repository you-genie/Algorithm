   "source": [
    "from collections import defaultdict\n",
    "\n",
    "\n",
    "class Solution:\n",
    "    def groupAnagrams(self, strs: List[str]) -> List[List[str]]:\n",
    "        anagram_dict = defaultdict(list)\n",
    "        for s in strs:\n",
    "            anagram_dict[str(sorted(s))].append(s)\n",
    "    \n",
    "        return list(anagram_dict.values())"
   ]
  }
